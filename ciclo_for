#include <stdio.h>
#include <stdlib.h>
/* Uso do For
Fazer um ciclo para escrever 20 vezes Hello World
Cristiane Oliveira
*/

int main()
{
    printf("╔═══════════════════════════════════════════╗\n");
    printf("║         Ciclo FOR para Repetições         ║\n");
    printf("╚═══════════════════════════════════════════╝\n");

    for(int i=1;i<21;i++){
        printf("%d - Hello world!\n",i);
    }
    return 0;
}
